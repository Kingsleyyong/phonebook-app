{
      "version": 2,
      "builds": [
            {
                  "src": "main.py",
                  "use": "@vercel/python",
                  "config": {
                        "buildCommand": "pip install --no-cache-dir -r requirements.txt"
                  }
            }
      ],
      "routes": [
            {
                  "src": "/(.*)",
                  "dest": "main.py"
            }
      ]
}
    